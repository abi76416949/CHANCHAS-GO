<h1>Nuevo Propietario</h1>

<%= form_with model: @propietario, local: true do |form| %>
  <div>
    <%= form.label :nombre %>
    <%= form.text_field :nombre %>
  </div>

  <div>
    <%= form.label :polideportivo %>
    <%= form.text_field :polideportivo %>
  </div>

  <div>
    <%= form.label :contacto %>
    <%= form.text_field :contacto %>
  </div>

  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>

  <div>
    <%= form.submit 'Guardar' %>
  </div>
<% end %>
